slidesData=[{"time":0,"slide":"0","title":"About Me","content":"<h1>SyncSlide -- Revolutionizing Presentation Accessibility</h1><h2>About Me</h2><p><strong>Melody Shih</strong></p><ul>\n<li>Working as an Accessibility Tester</li>\n<li>Studying human computer interaction (HCI)</li>\n<li>Recently completed a diploma in Software Development</li>\n</ul>"},{"time":18.7,"slide":"1","title":"Introduction to the Problem","content":"<h1>SyncSlide -- Revolutionizing Presentation Accessibility</h1><h2>Introduction to the Problem</h2><ul>\n<li>pressing accessibility issue in academic, conference, and business settings</li>\n<li>with the most accessible Powerpoint slides provided in advance</li>\n<li>Difficulty for people with print disabilities:</li>\n<li>Follow presentations on projecting screen</li>\n<li>Inconvenient for presenters</li>\n<li>Remember to say \"next slide\"</li>\n</ul>"},{"time":47,"slide":"2","title":"What is SyncSlide?","content":"<h1>SyncSlide -- Revolutionizing Presentation Accessibility</h1><h2>What is SyncSlide?</h2><ul>\n<li>A Tool for Inclusive Presentations</li>\n<li>Creates simple, accessible, live slides</li>\n<li>Compatible with screen readers, specialized fonts, color inversion</li>\n<li>All audience follow along on their own device</li>\n</ul>"},{"time":67,"slide":"3","title":"Features of SyncSlide","content":"<h1>SyncSlide -- Revolutionizing Presentation Accessibility</h1><h2>Features of SyncSlide</h2><ul>\n<li>Real-time slide updates</li>\n<li>Record and sync with presentation videos</li>\n<li>Extensive Markdown and CSS support</li>\n<li>Insert math equations using LaTeX</li>\n</ul>"},{"time":78.5,"slide":"4","title":"Demo","content":"<h1>SyncSlide -- Revolutionizing Presentation Accessibility</h1><h2>Demo</h2><p><strong>Please pause the video to watch the following demo</strong></p><video width=\"100%\" height=\"20%\" controls playsinline><source src=\"screenShare.mp4\" type=\"video/mp4\">Your browser does not support the video tag.</video>"},{"time":81,"slide":"5","title":"Potential Enhancements","content":"<h1>SyncSlide -- Revolutionizing Presentation Accessibility</h1><h2>Potential Enhancements</h2><ul>\n<li>Add audience interaction tools</li>\n<li>Include keyboard shortcuts for common actions</li>\n<li>Provide templates of widely used elements in presentations</li>\n<li>Develop structured documentations and tutorials</li>\n</ul>"},{"time":98,"slide":"6","title":"Conclusion","content":"<h1>SyncSlide -- Revolutionizing Presentation Accessibility</h1><h2>Conclusion</h2><ul>\n<li>Goals of SyncSlide:\n<ul>\n<li>Revolutionizing Presentation Delivery</li>\n<li>Simplifies creation and updating of slides</li>\n<li>Enhances accessibility for all users</li>\n</ul></li>\n<li>Check out the prototype at <a href=\"https://syncslide.clippycat.ca/\">syncslide.clippycat.ca</a></li>\n<li>Contribute to the project on GitHub: <a href=\"https://github.com/ClippyCat/syncslide/\">github.com/ClippyCat/syncslide/</a></li>\n</ul>"}]